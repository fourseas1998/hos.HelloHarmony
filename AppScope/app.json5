//应用的全局的配置文件，用于存放应用公共的配置信息。
{
  "app": {
//    包名
    "bundleName": "com.example.helloworld",
//    应用程序供应商
    "vendor": "example",
//    用于区分应用版本
    "versionCode": 1000000,
//    版本号
    "versionName": "1.0.0",
//    应用的显示图标
    "icon": "$media:app_icon",
//    应用名
    "label": "$string:app_name",
//    描述应用程序是否已分发通知
    "distributedNotificationEnabled": true
  }
}
